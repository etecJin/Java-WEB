package br.com.Util;

/**
 *
 * @author Administrador
 */
public class TipoSO {

    public static boolean isLinux() {

        return System.getProperty("os.name").contains("Linux");
    }


    public static boolean isWindows() {
        
         return System.getProperty("os.name").contains("Windows");
    }
}
